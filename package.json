{
	"name": "nptucsss",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "node index.js",
		"dev": "concurrently \"yarn server:dev\" \"yarn client:dev\"",
		"server:dev": "nodemon",
		"server:start": "node index.js",
		"client:dev": "yarn --cwd ./client/ start",
		"client:build": "yarn --cwd ./client build",
		"init:server": "yarn",
		"init:client": "cd client && yarn",
		"init:deploy": ""
	},
	"dependencies": {
		"axios": "^0.21.1",
		"bcryptjs": "^2.4.3",
		"dotenv": "^9.0.2",
		"express": "^4.17.1",
		"express-validator": "^6.11.1",
		"jsonwebtoken": "^8.5.1",
		"mysql": "^2.18.1",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"concurrently": "^6.2.0",
		"nodemon": "^2.0.7"
	}
}
